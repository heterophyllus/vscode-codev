{
    "name": "vscode-codev",
    "displayName": "CodeV",
    "description": "CodeV Macro-plus language support for Visual Studio Code",
    "publisher": "heterophyllus",
    "author": {
        "name": "Hiiragi",
        "email": "heterophyllus.work@gmail.com"
    },
    "version": "0.5.3",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "CodeV",
        "CODE V",
        "interferometer",
        "CV-Interferograms"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/heterophyllus/vscode-codev.git"
    },
    "icon": "images/icon.png",
    "license": "MIT",
    "contributes": {
        "languages": [
            {
                "id": "codev",
                "aliases": [
                    "CodeV",
                    "codev",
                    "CODEV"
                ],
                "extensions": [
                    ".seq",
                    ".SEQ"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "codev-int",
                "aliases": [
                    "CodeV-INT",
                    "codev-int"
                ],
                "extensions": [
                    ".int",
                    ".INT"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "codev",
                "scopeName": "source.codev",
                "path": "./syntaxes/codev.tmLanguage.json"
            },
            {
                "language": "codev-int",
                "scopeName": "source.codev-int",
                "path": "./syntaxes/codev-int.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "codev",
                "path": "./snippets/codev.json"
            }
        ]
    }
}
